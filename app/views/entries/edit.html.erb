
<%= form_for @entry do |f| %>
  <%= f.label :author %>:
  <%= f.text_field :author %><br />

  <%= f.label :photo_url %>:
  <%= f.text_field :photo_url %><br />

  <%= f.label :date_taken %>:
  <%= f.text_field :date_taken %><br />

  <%= f.label :caption %>:
  <%= f.text_field :caption %><br />

  <%= f.submit %>
<% end %>

